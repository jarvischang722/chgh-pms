<!DOCTYPE html>
<html>

<%- include ../Common/eBoardHead.ejs %>

<body>

<%- include ../Common/eBoardHeader.ejs %>

<%- include ../Common/eBoardMarquee.ejs %>

<div class="container container_higher" id="out">
    <h1><span><i class="fa fa-check-square-o" aria-hidden="true"></i> 出院</span></h1>
    <div class="clearfix"></div>
    <table class="overflow-y">
        <thead>
        <tr>
            <th>病床</th>
            <th>病患</th>
            <th>備註</th>
        </tr>
        </thead>
        <tbody id="result_tbody">
        <tr v-for="result in results">
            <td>{{ result.bed_no | appendHyphenForBedNo}}</td>
            <td>
                <span v-if="result.sex=='男'" class="male">{{ result.patient_name }}({{result.age}})</span>
                <span v-if="result.sex=='女'" class="female">{{ result.patient_name }}({{result.age}})</span>
            </td>
            <td>{{ result.data_note }}</td>
        </tr>
        </tbody>
    </table>
    <div class="clearfix"></div>
</div>

<%- include('../Common/eBoardFooter', {func: 'dischargeNote'}) %>

<%- include ../Common/importJSFiles.ejs %>

</body>

</html>

<script type="text/javascript" src="../js/EWhiteBoard/dischargeNote.js"></script>