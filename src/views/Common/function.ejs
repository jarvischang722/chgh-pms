<%

    /*
    確認護理站是否有該模組的權限
     */
checkWardZoneHasModulePrivilege=function(moduleName){

    //console.log("可用模組:");
    //console.log(session.user.module_eng_name);

    if(moduleName!="EWhiteBoard" && moduleName!="Bit" && moduleName!="SIP"){

        return false;

    }else{

        if ((session.user.module_eng_name).indexOf(moduleName) > -1) {
            //In the array!
            return true;
        } else {
            //Not in the array
            return false;
        }

    }


}

%>